  <PERIPHERAL DESC="Timer" VALID="1" ENABLED="0" BUILTIN="1" TYPE="TIMER" ID="{62b98608-4dd3-4a6d-94b0-897cb965bba0}" NAME="timer" FILE="Raspberry Pi 4" EID="00000001" OPTIONAL="1">
