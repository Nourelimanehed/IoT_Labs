  <PERIPHERAL DESC="IOT MQTT" VALID="1" ENABLED="0" BUILTIN="1" TYPE="MQTT" ID="{29605684-ea6e-4408-9876-f37ab4e87643}" NAME="MQTT" FILE="Raspberry Pi 4" EID="00000001" OPTIONAL="1">
   <INTERFACE EXTERNAL="MQTT" OBJECT="MQTT">
    <CONFIG>
     <SETUP NAME="begin">
      <PROPERTY DESC="Unique Identifier for this MQTT Client" ELEMENT="U1" DEFAULT="Raspberry Pi 4" TYPE="STRING" ALIAS="clientID" NAME="MQTT_CLIENTID" EID="00000001" VALUE="Raspberry Pi 4"/>
      <PROPERTY DESC="IP address or DNS name of MQTT broker" ELEMENT="U1" DEFAULT="localhost" TYPE="STRING" ALIAS="server" NAME="MQTT_SERVER" EID="00000001" VALUE="localhost"/>
      <PROPERTY DESC="IP port of MQTT broker" ELEMENT="U1" DEFAULT="1883" TYPE="INTEGER" ALIAS="port" NAME="MQTT_PORT" EID="00000001" VALUE="1883"/>
      <PROPERTY DESC="Username for connect (optional)" ELEMENT="U1" TYPE="STRING" ALIAS="user" NAME="MQTT_USER" EID="00000001"/>
      <PROPERTY DESC="Password for connect (optional)" ELEMENT="U1" TYPE="STRING" ALIAS="password" NAME="MQTT_PASS" EID="00000001"/>
     </SETUP>
    </CONFIG>
   </INTERFACE>
   <DRIVER HEADER="MQTT">
    <ENUMERATION NAME="QOS">
     <CONSTANT NAME="Level_0" VALUE="0"/>
     <CONSTANT NAME="Level_1" VALUE="1"/>
     <CONSTANT NAME="Level_2" VALUE="2"/>
    </ENUMERATION>
    <INTERRUPT DESC="Connect Callback" HOOK="attachConnectHandler" NAME="onConnect"/>
    <INTERRUPT DESC="Message Callback" HOOK="attachMessageHandler" NAME="onMessage"/>
    <FUNCTION DESC="Return true if topic is the same as the callback">
     <PARAMETER DESC="Topic to check" TYPE="STRING" NAME="Topic"/>
     <RETURN TYPE="BOOLEAN"/>
    </FUNCTION>
    <METHOD DESC="Changes the name of the client" NAME="setClientId">
     <PARAMETER DESC="The new client Identifier" TYPE="STRING" NAME="clientId"/>
    </METHOD>
    <METHOD DESC="Add a message to for the client in the event of disconnection" NAME="setWillMessage">
     <PARAMETER DESC="Topic the will will be sent to" TYPE="STRING" NAME="topic"/>
     <PARAMETER DESC="QoS level of the will" DEFAULT="Level_0" TYPE="QOS" NAME="QoS"/>
     <PARAMETER DESC="If the will is remembered by the broker" DEFAULT="FALSE" TYPE="BOOLEAN" NAME="retain"/>
     <PARAMETER DESC="Message in the will" TYPE="LIST" NAME="will"/>
    </METHOD>
    <METHOD DESC="Add the MQTT broker and port connection details" NAME="connect">
     <PARAMETER DESC="Receave old messages" DEFAULT="TRUE" TYPE="BOOLEAN" NAME="cleanSession"/>
    </METHOD>
    <METHOD DESC="Subscribe to Topic" NAME="subscribe">
     <PARAMETER DESC="Topic you with to subscribe to" TYPE="STRING" NAME="topic"/>
     <PARAMETER DESC="QoS level within topic" DEFAULT="Level_0" TYPE="QOS" NAME="QoS"/>
    </METHOD>
    <METHOD DESC="Return Message" NAME="readMessage">
     <RETURN DESC="Last Message Receaved" DEFAULT="Message" TYPE="STRING" NAME="lastMessage"/>
    </METHOD>
    <METHOD DESC="Read the mqtt message list" NAME="readList">
     <RETURN DESC="Comma separated list of variables to receive list data" TYPE="LIST"/>
    </METHOD>
    <METHOD DESC="Return Message" NAME="readTopic">
     <RETURN DESC="Last Topic Read" DEFAULT="Topic" TYPE="STRING" NAME="lastMessageTopic"/>
    </METHOD>
    <METHOD DESC="Send message to Broker" NAME="publish">
     <PARAMETER DESC="Topic to send too" TYPE="STRING" NAME="topic"/>
     <PARAMETER DESC="Quality of Service level" DEFAULT="Level_0" TYPE="QOS" NAME="QoS"/>
     <PARAMETER DESC="Save in broker, and resend every time" DEFAULT="FALSE" TYPE="BOOLEAN" NAME="retain"/>
     <PARAMETER DESC="List of values to send" TYPE="LIST" NAME="message"/>
    </METHOD>
    <METHOD DESC="Unsubscribe from Topic" NAME="unsubscribe">
     <PARAMETER TYPE="STRING" NAME="topic"/>
    </METHOD>
    <METHOD DESC="Disconnect from MQTT broker" NAME="disconnect"/>
   </DRIVER>
